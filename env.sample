# .gimme_readme_config

[api_keys]
  # Make your OpenAI API Key: https://platform.openai.com/api-keys
  OPENAI_KEY = "YOUR_OPENAI_KEY_IN_QUOTATIONS"

  # Make your Gemini API Key: https://aistudio.google.com/app/apikey
  GEMINI_KEY= "YOUR_GEMINI_KEY_IN_QUOTATIONS"

  # Make your Groq API Key: https://console.groq.com/keys
  GROQ_KEY= "YOUR_GROQ_KEY_IN_QUOTATIONS"

[preferences]
  # Default model you prefer to use.
  # List of supported models: https://github.com/peterdanwan/gimme_readme#4-supported-models-by-providers
  MODEL = "gemini-2.0-flash"

  # Default temperature you prefer to use (0-1)
  TEMPERATURE = 0.5
